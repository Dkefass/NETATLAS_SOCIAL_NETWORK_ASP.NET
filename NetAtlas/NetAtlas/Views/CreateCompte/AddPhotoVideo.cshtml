@model NetAtlas.Views.PhotoVideoVM

@{
    ViewData["Title"] = "AddPhotoVideo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>AddPhotoVideo</h1>

<h4>PhotoVideoVM</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddPhotoVideo">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="nom_photovideo" class="control-label"></label>
                <input asp-for="nom_photovideo" class="form-control" />
                <span asp-validation-for="nom_photovideo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="photovideo_url" class="control-label"></label>
                <input asp-for="photovideo_url" class="form-control" />
                <span asp-validation-for="photovideo_url" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
